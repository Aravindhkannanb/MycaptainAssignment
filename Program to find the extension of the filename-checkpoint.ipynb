   "metadata": {},
   "outputs": [],
   "source": [
    "x = input(\"Input the file name : \")\n",
    "v = x.split(\".\")\n",
    "b = repr(v[1])\n",
    "\n",
    "print(\"The extension of the file {} is {}\".format(x,b))\n",
    "if(b[1:len(b)-1]=='py'):\n",
    "    print(\"The Extension of the file is : Python\")\n",
    "else:\n",
    "    print(\"bye\")\n"
   ]
  }
